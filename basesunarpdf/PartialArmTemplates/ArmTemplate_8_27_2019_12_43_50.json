{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "basesunarpdf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "CosmosDbMongoDbApiSink",
								"writeBatchTimeout": "00:30:00",
								"writeBehavior": "insert"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Llave",
											"type": "String"
										},
										"sink": {
											"path": "Llave"
										}
									},
									{
										"source": {
											"name": "TipoDocumento",
											"type": "String"
										},
										"sink": {
											"path": "TipoDocumento"
										}
									},
									{
										"source": {
											"name": "NumeroDocumento",
											"type": "String"
										},
										"sink": {
											"path": "NumeroDocumento"
										}
									},
									{
										"source": {
											"name": "ApellidosCliente",
											"type": "String"
										},
										"sink": {
											"path": "ApellidosCliente"
										}
									},
									{
										"source": {
											"name": "ApePaterno",
											"type": "String"
										},
										"sink": {
											"path": "ApePaterno"
										}
									},
									{
										"source": {
											"name": "ApeMaterno",
											"type": "String"
										},
										"sink": {
											"path": "ApeMaterno"
										}
									},
									{
										"source": {
											"name": "NombresCliente",
											"type": "String"
										},
										"sink": {
											"path": "NombresCliente"
										}
									},
									{
										"source": {
											"name": "PrimerNombre",
											"type": "String"
										},
										"sink": {
											"path": "PrimerNombre"
										}
									},
									{
										"source": {
											"name": "SegundoNombre",
											"type": "String"
										},
										"sink": {
											"path": "SegundoNombre"
										}
									},
									{
										"source": {
											"name": "Sexo",
											"type": "String"
										},
										"sink": {
											"path": "Sexo"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "DelimitedText1",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "CosmosDbMongoDbApiCollection1",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DelimitedText1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DelimitedText1')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureBlobStorage1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "EjemploCSV.csv",
						"container": "adftutorial"
					},
					"columnDelimiter": "|",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Llave",
						"type": "String"
					},
					{
						"name": "TipoDocumento",
						"type": "String"
					},
					{
						"name": "NumeroDocumento",
						"type": "String"
					},
					{
						"name": "ApellidosCliente",
						"type": "String"
					},
					{
						"name": "ApePaterno",
						"type": "String"
					},
					{
						"name": "ApeMaterno",
						"type": "String"
					},
					{
						"name": "NombresCliente",
						"type": "String"
					},
					{
						"name": "PrimerNombre",
						"type": "String"
					},
					{
						"name": "SegundoNombre",
						"type": "String"
					},
					{
						"name": "Sexo",
						"type": "String"
					},
					{
						"name": "NombreEstadoCivil",
						"type": "String"
					},
					{
						"name": "FechaNacimiento",
						"type": "String"
					},
					{
						"name": "MesaSufragio",
						"type": "String"
					},
					{
						"name": "CondicionLaboral",
						"type": "String"
					},
					{
						"name": "FlagCCC",
						"type": "String"
					},
					{
						"name": "FlagTCA",
						"type": "String"
					},
					{
						"name": "FlagFallecido",
						"type": "String"
					},
					{
						"name": "FlagVehicular",
						"type": "String"
					},
					{
						"name": "FlagHijos",
						"type": "String"
					},
					{
						"name": "NumeroHijos",
						"type": "String"
					},
					{
						"name": "UbigeoReniec",
						"type": "String"
					},
					{
						"name": "DepartamentoReniec",
						"type": "String"
					},
					{
						"name": "ProvinciaReniec",
						"type": "String"
					},
					{
						"name": "DistritoReniec",
						"type": "String"
					},
					{
						"name": "UbigeoInei",
						"type": "String"
					},
					{
						"name": "GradoInstruccion",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		}
	]
}